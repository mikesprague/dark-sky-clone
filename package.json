{
  "name": "skydark-app",
  "version": "0.1.0",
  "description": "Open source PWA built as an homage to the now defunct Dark Sky for Android app",
  "private": true,
  "engines": {
    "node": ">= 12.x",
    "npm": ">= 6.x",
    "yarn": ">= 1.22.x"
  },
  "scripts": {
    "build": "yarn webpack && yarn lambda:build",
    "build:local": "yarn clean && yarn webpack && yarn lambda:build",
    "clean": "rm -rf ./build && rm -rf ./lambda",
    "lambda:build": "netlify-lambda build ./src/functions",
    "lambda:serve": "netlify-lambda serve ./src/functions --config ./functions.config.js",
    "prepublish": "yarn snyk",
    "snyk": "snyk protect",
    "start": "concurrently 'yarn webpack:serve' 'yarn lambda:serve' --names 'ðŸ“¦,ðŸ’»' --prefix '[{name}] ' --kill-others",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "cross-env NODE_ENV=production webpack --color",
    "webpack:dev": "cross-env NODE_ENV=development webpack --color",
    "webpack:serve": "cross-env NODE_ENV=development webpack serve"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/mikesprague/skydark-app"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "homepage": "https://skydark.app",
  "license": "MIT",
  "dependencies": {
    "@bugsnag/core": "^7.7.0",
    "@bugsnag/js": "^7.8.0",
    "@bugsnag/plugin-react": "^7.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^6.0.0-alpha2",
    "@fortawesome/free-regular-svg-icons": "^6.0.0-alpha2",
    "@fortawesome/free-solid-svg-icons": "^6.0.0-alpha2",
    "@fortawesome/pro-duotone-svg-icons": "^6.0.0-alpha2",
    "@fortawesome/pro-light-svg-icons": "^6.0.0-alpha2",
    "@fortawesome/pro-regular-svg-icons": "^6.0.0-alpha2",
    "@fortawesome/pro-solid-svg-icons": "^6.0.0-alpha2",
    "@fortawesome/pro-thin-svg-icons": "^6.0.0-alpha2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@tailwindcss/typography": "^0.4.0",
    "@tippyjs/react": "^4.2.3",
    "axios": "^0.21.1",
    "cogo-toast": "^4.2.3",
    "dayjs": "^1.10.4",
    "history": "^5.0.0",
    "leaflet": "^1.7.1",
    "nanoid": "^3.1.20",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-google-charts": "^3.0.15",
    "react-hot-loader": "^4.13.0",
    "react-leaflet": "^3.1.0",
    "react-router-dom": "^6.0.0-beta.0",
    "register-service-worker": "^1.7.2",
    "snyk": "^1.476.0",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/eslint-plugin": "^7.13.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.13.9",
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "@hot-loader/react-dom": "^17.0.1",
    "@octokit/core": "^3.2.5",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "compression-webpack-plugin": "^7.1.2",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-auto": "^0.6.6",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.0",
    "mini-css-extract-plugin": "^1.3.9",
    "netlify-cli": "^3.10.6",
    "netlify-lambda": "^2.0.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.7",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^5.1.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-stylelint": "^0.4.2",
    "rollup": "^2.40.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "3.11.2",
    "workbox-webpack-plugin": "^6.1.1"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-object-assign",
      "@babel/plugin-transform-runtime",
      "add-react-displayname",
      "react-hot-loader/babel"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "jsx-a11y",
      "markdown",
      "prettier",
      "react",
      "react-hooks",
      "security"
    ],
    "parserOptions": {
      "ecmaVersion": 11,
      "allowImportExportEverywhere": true,
      "parser": "babel-parser",
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "react/jsx-boolean-value": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-onchange": "off",
      "consistent-return": "off"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabwidth": true,
    "trailingComma": "all"
  }
}
